{
  "name": "trianity/laravel-otp",
  "license": "MIT",
  "description": "OTP Generator and Validator for Laravel Applications",
  "keywords": [
    "laravel",
    "otp",
    "receipt"
  ],
  "version": "10.0.1",
  "authors": [
    {
      "name": "Holbok Istv√°n",
      "email": "holbok@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0|^8.1",
    "illuminate/support": "^9.32|^10.0"
  },
  "require-dev": {
    "laravel/pint": "^1.2",
    "nunomaduro/larastan": "^2.0",
    "nunomaduro/phpinsights": "^2.6",
    "orchestra/testbench": "^7.0|^8.0",
    "pestphp/pest": "^2.6"
  },
  "autoload": {
    "psr-4": {
      "Trianity\\Otp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Trianity\\Otp\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Trianity\\Otp\\Providers\\PackageServiceProvider"
      ],
      "aliases": {
        "Otp": "Trianity\\Otp\\Facades\\Otp"
      }
    }
  },
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
